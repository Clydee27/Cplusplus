/*
Read 3 numbers from stdin and print their sum to stdout.

Input Format
  - A single line containing 3 space-separated integers: a,b, and c.
Output
Print the sum of the three numbers on a single line.
*/
#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int a=0, b=0, c=0,sum=0;
    cin>>a>>b>>c;
    sum=a+b+c;
    
    cout<<sum;
    return 0;
}
